<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="180.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.ac.fhcampus.simple_manager.Controllers.WarningController">
    <children>
        <VBox alignment="TOP_CENTER" layoutX="20.0" layoutY="20.0" prefHeight="140.0" prefWidth="360.0" spacing="15.0">
            <children>
                <Label prefWidth="360.0" styleClass="section-label" stylesheets="@css/Style.css" text="Are you sure you want to delete this entry?" wrapText="true" />
                <Label fx:id="entryInfoLabel" prefWidth="360.0" text="Entry Info" wrapText="true" />
                <HBox alignment="CENTER" prefWidth="360.0" spacing="20.0">
                    <children>
                        <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#handleCancel" prefHeight="35.0" prefWidth="120.0" text="Cancel" />
                        <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#handleDelete" prefHeight="35.0" prefWidth="120.0" text="Delete" />
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
